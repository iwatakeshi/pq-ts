{
  "name": "pq-ts",
  "version": "0.1.0",
  "description": "A priority queue written in TypeScript.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "main.ts",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup main.ts --dts --format esm,cjs --sourcemap",
    "test": "vitest",
    "docs:generate": "deno doc --html --name=pq-ts main.ts",
    "docs:serve": "npx serve docs",
    "prepublishOnly": "npm run build && npm run docs:generate"
  },
  "keywords": [
    "priority-queue",
    "typescript",
    "browser",
    "node"
  ],
  "author": {
    "name": "Takeshi Iwana",
    "email": "iwatakeshi@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iwatakeshi/pq-ts.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.3.5",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
